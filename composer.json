{
  "name": "arillo/silverstripe-elements",
  "description": "Elements",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/arillo/silverstripe-elements",
  "keywords": ["silverstripe", "elements"],
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Murcia",
      "email": "dm@arillo.net"
    },
    {
      "name": "Sebastian Fischer",
      "email": "sf@arillo.net"
    }
  ],
  "require": {
    "silverstripe/framework": "^4",
    "arillo/silverstripe-dataobject-preview": "dev-master",
    "symbiote/silverstripe-gridfieldextensions": "^3",
    "colymba/gridfield-bulk-editing-tools": "dev-master"
  },
  "repositories": [
      {
          "type": "vcs",
          "url": "git@github.com:arillo-ch/silverstripe-gridfieldextensions.git"
      }
  ],
  "extra": {
    "installer-name": "elements",
    "expose": ["client"]
  },
  "autoload": {
    "psr-4": {
      "Arillo\\Elements\\": "src/"
    }
  },
  "support": {
    "issues": "https://github.com/arillo/silverstripe-elements/issues"
  }
